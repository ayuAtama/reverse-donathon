# How many seconds each unit represents (e.g. 540 = 9 minutes)
# Examples: 60 = 1 min, 90 = 1 min 30 sec, 300 = 5 min, 540 = 9 min
SECONDS_PER_UNIT=540

# Password to access the /admin panel
ADMIN_PASSWORD=your-secret-password
